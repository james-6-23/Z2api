{
  "compilerOptions": {
    "lib": ["deno.window"],
    "strict": true
  },
  "tasks": {
    "start": "deno run --allow-net --allow-env app.ts",
    "dev": "deno run --allow-net --allow-env --watch app.ts",
    "docker:build": "docker build -t deepinfra-proxy .",
    "docker:run": "docker run -p 8000:8000 deepinfra-proxy",
    "deploy": "./deploy.sh"
  }
}